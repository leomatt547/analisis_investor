<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="investors_investor_tree">
    <field name="name">investors.investor.tree</field>
    <field name="model">investors.investor</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="nama"/>
        <field name="alamat"/>
        <field name="email"/>
        <field name="pendapatan"/>
        <field name="current_asset"/>
        <field name="total_asset"/>
        <field name="current_liabilities"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="investors_investor_omzet_tree">
    <field name="name">investors.investor.omzet.tree</field>
    <field name="model">investors.investor.omzet</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="nama"/>
        <field name="alamat"/>
        <field name="email"/>
        <field name="pendapatan"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="investors_investor_dayabayar_tree">
